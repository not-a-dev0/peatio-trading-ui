<div class="col-lg-2" id="market_trades_wrapper" style="height:500px; padding:0px 2px; overflow-x: hidden; outline: none;">
  <div class="col-md-12" id="market_trades" style="background: #171e2e;height: 500px;padding:0px;">
    <div class="panel-group mlr-5">
      <p class="text-white mt-5 fs-11">TRADE HISTORY</p>
      <table class=" custom-market-tbl">
        <thead>
          <tr>
            <th scope="col">Price(BTC)</th>
            <th scope="col">Amount(USD)</th>
            <th scope="col" class="text-right">Time</th>
          </tr>
        </thead>
      </table>
    </div>
    <div class="mlr-5 panel-body panel-body-content">
      <table class="table table-hover all-trades custom-market-tbl" cellspacing="0" cellpadding="0" style="border: none;">
        <tbody></tbody>
      </table>
      <table class="table table-hover my-trades">
        <tbody></tbody>
      </table>
    </div>
  </div>
</div>
